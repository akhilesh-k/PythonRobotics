    "def intFromString(a_string):\n",
    "    try:\n",
    "        return int(a_string)\n",
    "    except ValueError:\n",
    "        print (\"Oops! Couldn't convert the string to an integer.\")\n",
    "        return None\n",
    "\n",
    "a = intFromString(4)\n",
    "if a:\n",
    "    print(\"a\")\n",
    "\n",
    "b = intFromString(\"hallo\")\n",
    "if b:\n",
    "    print(\"b\")"
   ]
